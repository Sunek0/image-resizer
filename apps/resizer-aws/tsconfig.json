{
	"compilerOptions": {
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"target": "es5",
		"outDir": ".build",
		"moduleResolution": "node",
		"lib": ["es2020"],
		"rootDir": "./",
		"noImplicitAny": false,
		"esModuleInterop": true
	},
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}
